local pGui = game.Players.LocalPlayer.PlayerGui

local player = game.Workspace.Dunimo113

local screengui = Instance.new("ScreenGui") -- create screen gui
 screengui.Parent = pGui

local frame = Instance.new("Frame") -- create frame
 frame.Parent = pGui.ScreenGui
 frame.Size = UDim2.new(0, 419,0, 234)
 frame.Position = UDim2.new(0.294, 0,0.227, 0)
 frame.BackgroundColor3 = Color3.fromRGB(45, 45, 45)

local button1 = Instance.new("TextButton") -- textbutton
 button1.Parent = frame
 button1.Name = "Button1"
 button1.Position = UDim2.new(0.022, 0,0.83, 0)
 button1.Size = UDim2.new(0, 87,0, 31)                          --Button1
 button1.BackgroundColor3 = Color3.fromRGB(0, 68, 5)
 button1.TextSize = 10
 button1.TextColor3 = Color3.fromRGB(255, 255, 255)
 button1.Text = "SuperSpeed"

local script1 = Instance.new("Script")
 script1.Parent = button1                     --script 1
 script1.Name = "Script1"
 
local button1 = script1.Parent
 button1.Activated:Connect(function()                 --command 1 (superspeed)
	player.Humanoid.WalkSpeed = 50 -- here your username
end)

local ExitButton = Instance.new("TextButton")        -- ExitButton
 ExitButton.Parent = frame
 ExitButton.Name = "ExitButton"
 ExitButton.Position = UDim2.new(0.92, 0,0.02, 0)
 ExitButton.Size = UDim2.new(0, 28,0, 31)       
 ExitButton.BackgroundColor3 = Color3.fromRGB(68, 1, 1)
 ExitButton.Text = "X"
 ExitButton.TextSize = 17
 ExitButton.TextColor3 = Color3.fromRGB(255, 255, 255)
 
local exitscript = Instance.new("LocalScript")
 exitscript.Parent = ExitButton
 exitscript.Name = "Exitscript"
 
local ExitButton = exitscript.Parent
ExitButton.Activated:Connect(function()                 --ExitScript
	screengui.Enabled = false
	end)

local screengui2 = Instance.new("ScreenGui") -- create screen gui 2
 screengui2.Parent = pGui
 screengui2.Name = "ScreenGui2"


local openbutton = Instance.new("TextButton") 
 openbutton.Parent = screengui2
 openbutton.Text = "GUI"                                    --open button
 openbutton.Position = UDim2.new(0.85, 5,0, 0)
 openbutton.Size = UDim2.new(0, 28,0, 31)
 openbutton.BackgroundColor3 = Color3.fromRGB(0, 57, 9)
 openbutton.TextSize = 9
 openbutton.TextColor3 = Color3.fromRGB(255, 255, 255)
  
local opescript = Instance.new("LocalScript")
 opescript.Parent = openbutton
 opescript.Name = "openscript"
 
 local openbutton = opescript.Parent
 openbutton.Activated:Connect(function()               --open script
	screengui.Enabled = true
end)

local sitbutton = Instance.new("TextButton")                    -- sitbutton
 sitbutton.Parent = frame
 sitbutton.Name = "Sit"
 sitbutton.Position = UDim2.new(0.02, 0,0.64, 0)
 sitbutton.Size = UDim2.new(0, 48,0, 31) 
 sitbutton.Text = "Sit"
 sitbutton.TextSize = 10
 sitbutton.BackgroundColor3 = Color3.fromRGB(0, 68, 5)
 sitbutton.TextColor3 = Color3.fromRGB(255, 255, 255)
  
  local sitscript = Instance.new("LocalScript")
 sitscript.Parent = sitbutton                          --sit script create
 sitscript.Name = "Sitscript" 

local sitbutton = sitscript.Parent
sitbutton.Activated:Connect(function()                 --sit script
	player.Humanoid.Sit = true
end)
 
